<?xml version="1.0" encoding="UTF-8"?>

        <!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.example.its.database.icrelation.ICRelationDBMapper">

    <insert id="createICRelation" useGeneratedKeys="true" keyProperty="ID">
        insert INTO ICRelationDB(issueID, commentID)
        values (#{issueID}, #{commentID})
    </insert>
</mapper>